{
  "name": "express-react-router-example",
  "version": "2.3.1",
  "description": "A simple website that uses express-react-router",
  "author": "Nick Heyn <nick@heyn.io>",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/nheyn/express-react-router"
  },
  "license": "MIT",
  "scripts": {
    "babel": "babel ./src -d ./build",
    "browserify": "node browserify.js ./build/client.js ./build/app.js",

    "build": "npm run babel && npm run browserify",
    "server": "node ./build/server.js",

    "postinstall": "npm run build",
    "start": "npm run server"
  },
  "engines": {
    "node": "v5"
  },
  "dependencies": {
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "babel-cli": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browserify": "^13.0.1",
    "express-react-router": "^2.4.0"
  }
}
